<html>
<head>
<title>User Dashboard | All</title>
<link rel="shortcut icon" type="image/png" href="./assets/img/favicon.ico"/>
<link rel="stylesheet" href="./assets/css/bootstrap.min.css" type="text/css" />
<script src="/assets/js/jquery.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<link rel="stylesheet" href="assets/css/styles.css" />
<script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
<script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
</head>
<body>

    <table style="width: 100%;">
        <tr>
            <a href="/create" class="btn btn-primary">Create</a>
        </tr>

        <tr>
            <th>id</th>
            <th>Username</th>
            <th>Password</th>
            <th>Role</th>
            <th>action</th>
        </tr>
    
        <% users.forEach(i => { %>
            <tr>
                <td><%= i.id %></td>
                <td><%= i.username %></td>
                <td><%= i.password %></td>
                <td><%= i.role %></td>
            
            <td>
                <a href="/edit/<%= i.id %>" class="btn btn-primary">Edit</a>
                <a href="/delete/<%= i.id %>" class="btn btn-warning">Delete</a>
            </td>
            </tr>
            <% }) %>

            <tr>
                <a href="/logout" class="btn btn-primary">Logout</a>
            </tr>
    </table>


</body>
</html>